x = input('Digite algo: ')
print('O tipo primitivo desta palavra é: {}'.format(type(x)))
print('{} só tem espaços? {}'.format(x, x.isspace()))
print('{} é um número? {}'.format(x, x.isnumeric()))
print('{} é uma palavra? {}'.format(x, x.isalpha()))
print('{} é alfanumérico? {}'.format(x, x.isalnum()))
print('{} está totalemente em maiúsculo? {}'.format(x, x.isupper()))
print('{} está totalmente em minúsculo? {}'.format(x, x.islower()))
print('{} está capitalizado? {}'.format(x, x.istitle()))
