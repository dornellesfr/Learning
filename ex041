from datetime import date
nascimento = int(input('Digite o seu ano de nascimento: '))
idade = date.today().year - nascimento
if idade <= 9:
    print('Com a sua idade de {} anos você deve participar da equipe MIRIM.'.format(idade))
elif idade <= 14:
    print('Com a sua idade de {} anos você deve participar da equipe INFANTIL'.format(idade))
elif idade <= 19:
    print('Com a sua idade de {} anos você deve participar da equipe JUNIOR'.format(idade))
elif idade <= 25:
    print('Com a sua idade de {} anos você deve participar da equipe SÊNIOR'.format(idade))
elif idade > 25:
    print('Com a sua idade de {} anos você deve participar da equipe MASTER'.format(idade))
